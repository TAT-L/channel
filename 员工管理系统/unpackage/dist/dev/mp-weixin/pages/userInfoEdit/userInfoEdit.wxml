<view class="page"><view class="title"><text>个人信息编辑</text></view><view class="information "><view class="box name"><label class="label" for="name">姓名：</label><input class="input" type="text" value="{{userInfo.name}}" id="name" placeholder="填写真实姓名" confirm-type="next" cursor-spacing="100" confirm-hold="true" maxlength="12" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['userInfo']]]]]}}" bindinput="__e"/></view><view class="phoneNumber box"><label class="label" for="phoneNumber">电话：</label><input class="input" type="digit" value="{{userInfo.phone}}" id="phoneNumber" cursor-spacing="100" confirm-type="next" placeholder="请填写你的电话" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['userInfo']]]]]}}" bindinput="__e"/></view><block wx:if="{{warning!=''}}"><view class="note">{{warning}}</view></block><button class="save" type="default" hover-class="save_hover" data-event-opts="{{[['tap',[['changeWorkerInfo',['$event']]]]]}}" bindtap="__e">保存</button></view></view>