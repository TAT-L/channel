<view class="page"><block wx:if="{{status===1}}"><view class="tips"><text>注册成功，请等待管理员审核</text></view></block><block wx:if="{{status!=1}}"><view class="form"><view class="name box"><label>姓名：</label><input class="nameInput input" type="text" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="phoneNumber box"><label>电话：</label><input class="phoneNumberInput input" type="text" data-event-opts="{{[['input',[['__set_model',['','userPhoneNumber','$event',[]]]]]]}}" value="{{userPhoneNumber}}" bindinput="__e"/></view><view class="picker box"><picker value="{{index}}" range="{{managerList}}" range-key="name" data-event-opts="{{[['change',[['pickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{managerList[index].name}}</view></picker></view><button class="registerButton" type="default" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button></view></block></view>